@misc{MTMT:2241693,
 abstract = {Egy statisztikai teszt robusztussága azt jelenti, hogy érvényes (valid) marad akkor is, ha alkalmazásának feltételei nem teljesülnek. Tanulmányunkban számítógép segítségével nagyon sok mintát generálunk (amelyek beállításunk szerint az alkalmazási feltételeknek megfelelő, vagy azt irányítottan megsértő sokaságból származnak), elvégezzük a teszteket, és összehasonlítjuk a tapasztalt elsőfajú hibaarányt a választott szignifikancia-szinttel. A robosztusság vizsgálatának ez az ún. Monte Carlo szimulációs módszere (főképp a nagyszámú véletlenszám-generálás miatt) rendkívül számításigényes, hagyományosan csak szuperszámítógépekkel hajtható végre hatékonyan. Ugyanakkor egy új megközelítés, a GP-GPU módszer lehetővé teszi, hogy mindezt szokásos, bárki számára elérhető grafikus kártyák segítségével nagyságrendekkel rövidebb idő alatt elvégezzük.},
 author = {Ferenci, Tamás and Kotosz, Balázs Gyula},
 orcid-numbers = {Ferenci, Tamás/0000-0001-6791-3080},
 title = {Statisztikai tesztek robusztusságának vizsgálata GP-GPU számítási módszerrel},
 unique-id = {2241693},
 url = {https://m2.mtmt.hu/api/publication/2241693},
 year = {2010}
}

